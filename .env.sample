# mongo
MONGO_IMAGE_VERSION=4.2
MONGO_INITDB_ROOT_USERNAME=mongo
MONGO_INITDB_ROOT_PASSWORD=test

# mongo-express
MONGO_EXPRESS_IMAGE_VERSION=
